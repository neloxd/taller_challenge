<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:color="@color/white_sigo" android:state_enabled="false">
        <shape>
            <solid android:color="@color/white_sigo" />
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item android:color="@color/white_sigo" android:state_focused="true"  android:state_activated="true">
        <shape>
            <solid android:color="@color/white_sigo" />
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item android:color="@color/white_sigo"  android:state_pressed="true">
        <shape>
            <solid android:color="@color/white_sigo" />
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item android:color="@color/white_sigo" android:state_enabled="true">
        <shape>
            <solid android:color="@color/white_sigo" />
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item android:color="@color/white_sigo">
        <shape>
            <solid android:color="@color/white_sigo" />
            <corners android:radius="4dp" />
        </shape>
    </item>
</selector>